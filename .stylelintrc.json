{
  "extends": ["stylelint-config-standard", "stylelint-config-prettier"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "function",
          "if",
          "for",
          "each",
          "mixin",
          "include",
          "extend",
          "-webkit-keyframes"
        ]
      }
    ]
  }
}
